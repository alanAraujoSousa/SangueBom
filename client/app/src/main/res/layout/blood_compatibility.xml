<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    >



    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/title_compatibilidade"
        android:background="@drawable/compatibilidade"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tabela_compatibilidade"
        android:background="@drawable/tabela_compatibilidade"
        android:layout_below="@id/title_compatibilidade"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        />





</RelativeLayout>